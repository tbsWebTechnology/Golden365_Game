<script>
   function formatIndianNumber(num) {
      let formattedNum = new Intl.NumberFormat("en-IN", {
         maximumFractionDigits: 2,
         minimumFractionDigits: 2,
      }).format(num)
      return formattedNum
   }
   function getOrdinal(n) {
      let s = ["th", "st", "nd", "rd"],
         v = n % 100
      return n + (s[(v - 20) % 10] || s[v] || s[0])
   }

   const initAttendanceBonus = async () => {
      try {
         const response = await axios.get("/api/activity/attendance_bonus")

         if (response.data.status === true) {
            $("#attendance_days_display").html(getOrdinal(response?.data?.data?.days || 0))
         }

         const response2 = await axios.get("/api/webapi/GetUserInfo")

         if (response2.data.status === true) {
            $("#accumulated_amount_display").html(`₹${formatIndianNumber(response2?.data?.data?.money_user)}`)
         }
      } catch (error) {
         console.log(error)
      }
   }

   initAttendanceBonus()

   const claimBonus = async () => {
      try {
         const response = await axios.post("/api/activity/attendance_bonus/claim")

         if (response.data.status === true) {
            await initAttendanceBonus()
            $(".van-toast__text").text(response.data.message)
            $("#successTost").fadeIn(220)
         } else {
            $(".van-toast__text").text(response.data.message)
            $("#failedTost").fadeIn(220)
         }

         setTimeout(() => {
            $("#failedTost").fadeOut()
            $("#successTost").fadeOut()
         }, 3000)
      } catch (error) {
         console.log(error)
         if (error.response.data.status === false) {
            $(".van-toast__text").text(error.response.data.message)
            $("#failedTost").fadeIn(220)

            setTimeout(() => {
               $("#failedTost").fadeOut()
               $("#successTost").fadeOut()
            }, 1600)
         }
      }
   }
</script>


