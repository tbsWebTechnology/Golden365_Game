 /* Basic popup styling */
 #popupdeposit {
            display: none;
            position: fixed;
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
            padding: 20px;
            background-color: white;
            border: 2px solid #ccc;
            z-index: 1000;
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        }

        /* Overlay for background dim */
        #popup-overlay {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 999;
        }

        /* Close button styling */
        .close-btn {
            cursor: pointer;
            color: red;
            font-weight: bold;
            float: right;
        }

      /* Container for the marquee effect */
    .noticeBar__container {
      width: 100%;
      background-color: #f0f0f0; /* Optional, for styling */
      padding: 10px;
      display: flex;
      align-items: center;
      justify-content: space-between;
    }

    /* Container for the text */
    .noticeBar__container-body {
      overflow: hidden;
      white-space: nowrap;
      width: 80%; /* Adjust width as needed */
    }

    /* The moving text */
    #noticehome {
      display: inline-block;
      animation: marquee 30s linear infinite; /* Adjust speed with the duration (10s) */
      padding-left: 100%; /* Start text off-screen on the right */
    }

    /* Keyframes to move text from right to left */
    @keyframes marquee {
      0% {
        transform: translateX(1%); /* Starts from the right edge */
      }
      100% {
        transform: translateX(-100%); /* Ends at the left edge */
      }
    }

    /* Style for the button */
    .hotIcon {
      background-color: #007bff; /* Button color */
      color: white;
      padding: 10px;
      border: none;
      cursor: pointer;
    }
  </style>



