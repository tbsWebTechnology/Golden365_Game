   <script>
      function scrollToSection(hash) {
  // First, navigate to the section (the browser scrolls automatically to the section)
  location.href = hash;

  // Add a slight delay to ensure the browser scrolls first, then apply the custom offset
  setTimeout(function() {
    const offset = 150; // Offset for the sticky header
    const targetElement = document.querySelector(location.hash);

    if (targetElement) {
      // Calculate the final position with the offset
      const elementPosition = targetElement.getBoundingClientRect().top + window.pageYOffset;
      const offsetPosition = elementPosition - offset;

      // Scroll to the adjusted position
      window.scrollTo({
        top: offsetPosition,
        behavior: "smooth"
      });
    }
  }, 100); // Delay in milliseconds
}

