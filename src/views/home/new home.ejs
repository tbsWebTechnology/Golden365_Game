     <script>
      document.addEventListener("DOMContentLoaded", function () {
  const tabs = document.querySelectorAll('.van-tab');
  const tabPanels = document.querySelectorAll('.van-tab__panel-wrapper');

  // Function to handle tab click
  function handleTabClick(event) {
    // Remove active class from all tabs and tab panels
    tabs.forEach(tab => tab.classList.remove('van-tab--active'));
    tabPanels.forEach(panel => {
      panel.classList.remove('active');
      panel.style.display = 'none'; // Ensure panels are hidden
    });

    // Add active class to the clicked tab
    const clickedTab = event.currentTarget;
    clickedTab.classList.add('van-tab--active');

    // Show the corresponding tab panel by adding the 'active' class
    const tabId = clickedTab.getAttribute('aria-controls');
    const activePanel = document.getElementById(tabId);
    activePanel.classList.add('active');
    activePanel.style.display = 'block'; // Ensure the correct panel is displayed
  }

  // Attach event listeners to each tab
  tabs.forEach(tab => {
    tab.addEventListener('click', handleTabClick);
  });

  // Set the first tab and panel as active initially
  tabs[0].classList.add('van-tab--active');
  tabPanels[0].classList.add('active');
  tabPanels[0].style.display = 'block';
});

     </script>

     

