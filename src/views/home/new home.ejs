      <script>
      const userInfoTypes = {
         total_recharge: "total_recharge", // Assuming total recharge refers to the user's deposit
      };
      
      let userData = null;
      let userDataFull = null;
      
      // Function to fetch user data (including deposit)
      const fetchUser = async () => {
         try {
            const response = await axios.get("/api/webapi/GetUserInfo");
            userData = response?.data?.data;
            userDataFull = response?.data;
         } catch (error) {
            console.log("Error fetching user data:", error);
         }
      };
      
      // Function to update the popup visibility based on user deposit
      const handlePopup = (userDeposit) => {
          const popup = document.getElementById('popup'); // Reference to the popup element
      
          if (userDeposit >= 100) {
              // Hide the popup if deposit is 100 or more
              if (popup) {
                  popup.style.display = 'none';
              }
          } else {
              // Show the popup if deposit is less than 100
              if (popup) {
                  popup.style.display = 'block'; // Ensure the popup is visible
              }
          }
      };
      
      // Initialize function: fetch user info and handle popup visibility
      const initializePopup = async () => {
         await fetchUser(); // Fetch the user data first
      
         // Check if the user data is available
         if (!userData) {
            console.log("No user data available");
            return;
         }
      
         // Extract user deposit dynamically from fetched data
         const userDeposit = parseFloat(userData[userInfoTypes.total_recharge]) || 0;
      
         // Call the function to handle popup visibility
         handlePopup(userDeposit);
      };
      
      // Call initialize function on page load
      window.onload = function () {
         initializePopup();
      };
      </script>

