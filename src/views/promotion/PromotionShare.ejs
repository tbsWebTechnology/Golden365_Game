<script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js"></script>


<script>
  document.addEventListener('DOMContentLoaded', function () {
    // List of slide IDs and corresponding download buttons
    const slides = [
        { slideId: "share0", buttonId: "download-content-0" },
        { slideId: "share1", buttonId: "download-content-1" },
        { slideId: "share2", buttonId: "download-content-2" }
    ];

    // Function to capture slide content and download as image
    const captureAndDownloadSlide = (slideId) => {
        const slideElement = document.getElementById(slideId);
        if (!slideElement) {
            console.error(`Slide with ID '${slideId}' not found.`);
            return;
        }

        html2canvas(slideElement).then((canvas) => {
            const imageURL = canvas.toDataURL("image/png"); // Convert to PNG image
            const link = document.createElement("a");
            link.href = imageURL;
            link.download = `${slideId}.png`; // File name for the download
            link.click(); // Trigger download
        }).catch((error) => {
            console.error(`Error capturing slide '${slideId}':`, error);
        });
    };

    // Attach click event listeners to download buttons
    slides.forEach(({ slideId, buttonId }) => {
        const button = document.getElementById(buttonId);
        if (button) {
            button.addEventListener("click", () => captureAndDownloadSlide(slideId));
        } else {
            console.error(`Download button with ID '${buttonId}' not found.`);
        }
    });
});

</script>






<script>
  
  document.addEventListener('DOMContentLoaded', function () {
    const slides = document.querySelectorAll('.swiper-slide');
    const swiperWrapper = document.querySelector('.swiper-wrapper');

    if (!swiperWrapper || slides.length === 0) {
        console.error('Swiper structure is incomplete.');
        return;
    }

    let startX = 0, endX = 0, currentIndex = 0;

    function updateActiveSlide(index) {
        const totalSlides = slides.length;

        // Adjust currentIndex to stay within bounds
        currentIndex = (index + totalSlides) % totalSlides;

        // Update transform for sliding effect
        const offset = -currentIndex * 100;
        swiperWrapper.style.transform = `translateX(${offset}%)`;
        swiperWrapper.style.transition = 'transform 0.3s ease-in-out';

        // Update active class
        slides.forEach((slide, idx) => {
            if (idx === currentIndex) {
                slide.classList.add('swiper-slide-active');
            } else {
                slide.classList.remove('swiper-slide-active');
            }
        });
    }

    function handleSwipeChange() {
        const deltaX = endX - startX;

        if (deltaX > 50) {
            // Swipe right
            updateActiveSlide(currentIndex - 1);
        } else if (deltaX < -50) {
            // Swipe left
            updateActiveSlide(currentIndex + 1);
        }
    }

    swiperWrapper.addEventListener('touchstart', (e) => {
        startX = e.touches[0].clientX;
    });

    swiperWrapper.addEventListener('touchmove', (e) => {
        endX = e.touches[0].clientX;
    });

    swiperWrapper.addEventListener('touchend', () => {
        handleSwipeChange();
    });

    // Initialize first slide
    updateActiveSlide(currentIndex);
});



</script>


