 <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
 <script>
    document.addEventListener('DOMContentLoaded', async function () {
    const userInfoTypes = { referCode: "code" };
    let userData = null;

    // Fetch user data
    const fetchUser = async () => {
        try {
            const response = await fetch("/api/webapi/GetUserInfo");
            if (!response.ok) throw new Error("Network response was not ok.");
            const data = await response.json();
            userData = data.data;
            console.log("Fetched user data:", userData);
        } catch (error) {
            console.error("Failed to fetch user data:", error);
        }
    };

// Generate QR Code
const generateQRCode = (referralCode, canvasId) => {
    const qrCodeElement = document.getElementById(canvasId); // Target the specific canvas by its id
    if (!qrCodeElement) {
        console.error(`QR Code container element with id '${canvasId}' not found.`);
        return;
    }

    const invitationLink = `${window.location.origin}/register?invitationCode=${encodeURIComponent(referralCode)}`;
    console.log(`Generating QR Code with link: ${invitationLink} for ${canvasId}`);

    // Set QR Code size
    qrCodeElement.width = 135;
    qrCodeElement.height = 135;

    try {
        QRCode.toCanvas(qrCodeElement, invitationLink, {
            width: 135,
            height: 135,
        }, function (error) {
            if (error) {
                console.error(`Error generating QR Code for ${canvasId}:`, error);
                // Fallback: Try generating a Data URL instead
                QRCode.toDataURL(invitationLink, function (error, url) {
                    if (error) {
                        console.error(`Error generating QR Code Data URL for ${canvasId}:`, error);
                    } else {
                        console.log(`QR Code Data URL generated successfully for ${canvasId}!`);
                        console.log(`QR Code Data URL: ${url}`);
                    }
                });
            } else {
                console.log(`QR Code generated successfully for ${canvasId}!`);
            }
        });
    } catch (error) {
        console.error(`Unexpected error generating QR Code for ${canvasId}:`, error);
    }
};

// Fetch user data and generate QR codes for each slide
await fetchUser();
if (userData && userData[userInfoTypes.referCode]) {
    generateQRCode(userData[userInfoTypes.referCode], "qrcode-0");
    generateQRCode(userData[userInfoTypes.referCode], "qrcode-1");
    generateQRCode(userData[userInfoTypes.referCode], "qrcode-2");
} else {
    console.error("Referral code not available.");
}


    // Copy invitation link to clipboard
    const handleCopyLink = () => {
        if (!userData) {
            alert("User data is not available. Please try again later.");
            return;
        }

        const referralCode = userData[userInfoTypes.referCode];
        if (!referralCode) {
            alert("Referral code is not available. Please try again later.");
            return;
        }

        const invitationLink = `${window.location.origin}/register?invitationCode=${referralCode}`;
        console.log("Copying invitation link:", invitationLink);

        navigator.clipboard.writeText(invitationLink)
            .then(() => {
                const popup = document.getElementById("copy-success-popup");
                popup.classList.add("visible");
                setTimeout(() => popup.classList.remove("visible"), 2000); // Hide after 2 seconds
            })
            .catch((error) => {
                console.error("Failed to copy invitation link:", error);
                alert("Failed to copy invitation link. Please try again.");
            });
    };

    // Set up event listener for "Copy" button
    document.getElementById("sharelink").addEventListener("click", handleCopyLink);

    // Fetch user data and generate QR code
    await fetchUser();
    if (userData && userData[userInfoTypes.referCode]) {
        generateQRCode(userData[userInfoTypes.referCode]);
    } else {
        console.error("Referral code not available.");
    }
});


 </script>



