<script>
 document.addEventListener("DOMContentLoaded", function() {
const captchaContainer = document.getElementById('captcha');

function initializeCaptcha() {
  // Reset the CAPTCHA content before initializing
  captchaContainer.innerHTML = ''; // Clear previous CAPTCHA content
  
  sliderCaptcha({
      id: 'captcha',
      width: 340,
      height: 200,
      sliderL: 42,
      sliderR: 9,
      offset: 5,
      loadingText: 'Loading...',
      failedText: 'Try again',
      barText: 'Hold and slide',
      repeatIcon: 'fa fa-redo',
      setSrc: function () {
          // Optional: Set CAPTCHA source here
      },
      onSuccess: function () {
         captchaContainer.style.display = 'none'; // Hide CAPTCHA container on fail
document.getElementById('captchaoverlay').style.display = 'none'; // Hide overlay on fail
// Trigger login process
let phoneNumber = $("#phone_number_inp").val().trim();
let password = $("#password_inp").val().trim();
let dialCode = $('.dropdown__value span').text().trim()
$(".van-overlay").fadeIn(10);

$.ajax({
  type: "POST",
  url: "/api/webapi/login",
  data: {
   dialCode: dialCode,
   phoneNumber: phoneNumber,
      pwd: password,
  },
  dataType: "json",
  success: function (response) {
      if (response.status === true) {
          $(".van-overlay").fadeOut(300);
          $(".Loading").fadeIn(10);
          setCookie("token", response.token, 1);
          setCookie("auth", response.value, 1);
          setTimeout(() => {
              location.href = "/admin/manager/index";
          }, 500);
      } else {
          $(".van-overlay").fadeOut(300);
          alertMessage(response.message);
      }
  },
  error: function (error) {
      $(".van-overlay").fadeOut(300);
      alertMessage(error.responseJSON.message);
  },
});

$('.dropdown').click(function () {
         $('.dropdown__list').css("opacity", $('.dropdown__list').css("opacity") === "1" ? "0" : "1");
         $('.dropdown__list').css("height", $('.dropdown__list').css("opacity") === "1" ? ".2rem" : "6rem");
      });

      const setDialCode = (dialCode) => {
         $('.dropdown__value span').text(dialCode);
         $('.dropdown__list-item').removeClass('active');
         $(`.dropdown__list-item .dial_code:contains(${dialCode})`).parent().addClass('active');
      };


      $.ajax({
         url: "/country_codes.json",
         method: "GET",
         success: function (response) {
            // Handle success response

            const html = response.map((country) => {
               return `
                  <div data-v-482b13c8="" class="dropdown__list-item" onclick="setDialCode('${country.dial_code}')">
                     <span data-v-482b13c8="" class="dial_code">${country.dial_code}</span>
                     ${country.name}
                  </div>`;
            }).join('');

            $('.dropdown__list').html(html);

            setDialCode("+91");
         },
         error: function (xhr, status, error) {
            // Handle error response
         }
      });


},
      onFail: function () {
         captchaContainer.style.display = 'none'; // Hide CAPTCHA container on fail
document.getElementById('captchaoverlay').style.display = 'none'; // Hide overlay on fail
const dialog = document.getElementById('dialog');
dialog.style.display = 'block'; // Show dialog on fail
captchaContainer.innerHTML = ''; // Clear the CAPTCHA content

// Disable the showCaptcha button
const showCaptchaButton = document.getElementById('showCaptcha');
showCaptchaButton.disabled = true;

// Automatically close the dialog after 2 seconds
setTimeout(function() {
  dialog.style.display = 'none'; // Hide dialog
  captchaContainer.style.display = 'none'; // Hide CAPTCHA container
  document.getElementById('captchaoverlay').style.display = 'none'; // Hide overlay
  
  // Re-enable the showCaptcha button after dialog is closed
  showCaptchaButton.disabled = false;
}, 2000);
      },
      onRefresh: function () {
          // Handle refresh logic if needed
      }
  });
}

// Show CAPTCHA and initialize when the button is clicked
document.getElementById('showCaptcha').addEventListener('click', function() {
  captchaContainer.style.display = 'block'; // Show CAPTCHA
  document.getElementById('captchaoverlay').style.display = 'block'; // Show overlay
