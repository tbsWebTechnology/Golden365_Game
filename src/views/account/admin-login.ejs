   $(document).ready(function () {
 // Disable the showCaptcha button initially
 $("#showCaptcha").prop("disabled", true);

 // Update button state based on input
 function updateCaptchaButtonState() {
     const phoneNumber = $("#phone_number_inp").val().trim();
     const password = $("#password_inp").val().trim();
     const dialCode = $('.dropdown__value span').text().trim();
     $("#showCaptcha").prop("disabled", !phoneNumber || !password);
 }

 // Show the popup and handle click on the showCaptcha button
 $("#showCaptcha").click(async e => {
     e.preventDefault();
     let phoneNumber = $("#phone_number_inp").val().trim();
     let password = $("#password_inp").val().trim();
     let dialCode = $('.dropdown__value span').text().trim();

     // Validate fields
     if (!phoneNumber || !password || !dialCode) {
         alertMessage("Please fill in both fields."); // Show popup
         return; // Stop execution if fields are empty
     }

     // Proceed with showing the CAPTCHA if fields are filled
     showCaptcha();
 });

 // Attach input event listener to both fields
 $("#phone_number_inp, #password_inp").on('input', updateCaptchaButtonState);
});

</script>



